package net.minecraft.src;

public class SandbagBlock extends Block {
	protected SandbagBlock(int par1, int par2, Material par3Material) {
		super(par1, par2, Material.clay);
	}
	
    protected Block(int par1, Material par2Material) {
    	super(par1, Material.clay);
    }

	protected SandbagBlock(int par1, int par2) {
		super(par1, par2, null);
	}
}